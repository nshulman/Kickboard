        <defaultConnector>
            <targetReference>SendEmailToUserAction_Subflow</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>OutcomeIs_ExternalUserButNoCommunity</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>User_GetRecord</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
                >KickboardSetting_GetRecord.Guest_Url__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CannotActivate_Screen</targetReference>
            </connector>
            <label>OutcomeIs ExternalUserButNoCommunity</label>
        </rules>
        <rules>
            <name>OutcomeIs_CommunityUserButNoCommunity</name>
            <conditionLogic>1 AND (2 OR 3 OR 4) AND 5</conditionLogic>
            <conditions>
                <leftValueReference>User_GetRecord</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
                >User_GetRecord.Profile.UserType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>PowerPartner</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
                >User_GetRecord.Profile.UserType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>PowerCustomerSuccess</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
                >User_GetRecord.Profile.UserType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>CustomerSuccess</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference
                >KickboardSetting_GetRecord.Community_Url__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CannotActivate_Screen</targetReference>
            </connector>
            <label>OutcomeIs CommunityUserButNoCommunity</label>
        </rules>
    </decisions>
